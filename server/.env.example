# Copy this file to .env and fill in the real values (DO NOT commit secrets)

# Your Stripe Secret Key (starts with sk_live_ or sk_test_)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Your Stripe Webhook Signing Secret (from Stripe Dashboard)
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret_here

# Optional: port the server should listen on
PORT=3000
